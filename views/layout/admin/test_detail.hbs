<div class="col-md-8">
<h2>{{test.Title}}</h2>
<p>{{test.CourseTitle}}</p>
<div class="row">
    {{#if test.IsClosed}}
   <a href="/test/{{test._id}}/open"> <button class="btn btn-success" >Open Test</button></a>
    {{else}}
   <a href="/test/{{test._id}}/close"> <button class="btn btn-danger" >Close Test</button></a>  
    {{/if}}
</div>
</div>
 <hr>
 <div class="row">
                            <div class="col-md-6 col-xl-4">
                                <div class="card mb-3 widget-content">
                                    <div class="widget-content-outer">
                                        <div class="widget-content-wrapper">
                                            <div class="widget-content-left">
                                                <div class="widget-heading">Average Score</div>
                                                <div class="widget-subheading">In All tests</div>
                                            </div>
                                            <div class="widget-content-right">
                                                <div class="widget-numbers text-success">30</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-xl-4">
                                <div class="card mb-3 widget-content">
                                    <div class="widget-content-outer">
                                        <div class="widget-content-wrapper">
                                            <div class="widget-content-left">
                                                <div class="widget-heading">Attempts</div>
                                                <div class="widget-subheading">Currently Submitted</div>
                                            </div>
                                            <div class="widget-content-right">
                                                <div class="widget-numbers text-warning">{{getCount attemptData.attempts }}</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-xl-4">
                                <div class="card mb-3 widget-content">
                                    <div class="widget-content-outer">
                                        <div class="widget-content-wrapper">
                                            <div class="widget-content-left">
                                                <div class="widget-heading">People</div>
                                                <div class="widget-subheading">Emails</div>
                                            </div>
                                            <div class="widget-content-right">
                                                <div class="widget-numbers text-danger">350</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>                          
                        </div>
                        <hr>

 <div class="row">
                            <div class="col-md-12">
                                <div class="main-card mb-3 card">
                                    <div class="card-header">Attempts
                                    </div>
                                    <div class="table-responsive">
                                        <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                                            <thead>
                                            <tr>
                                                <th class="text-center">#</th>
                                                <th>Email</th>
                                                <th class="text-center">Score</th>
                                                <th class="text-center">Submitted</th>
                                                {{!-- <th class="text-center">Actions</th> --}}
                                            </tr>
                                            </thead>
                                    <tbody>
                                        {{#each attemptData.Attempts as |item i| }}
                                            <tr>
                                                <td class="text-center text-muted">#{{index i }}</td>
                                                <td>
                                                    <div class="widget-content p-0">
                                                        <div class="widget-content-wrapper">
                                                          {{item.Email}}
                                                        </div>
                                                    </div>
                                                </td>
                                                <td class="text-center">{{item.Score}}</td>
                                                <td class="text-center">
                                                    {{#if item.HasSubmitted}}
                                                    <div class="badge badge-success">Submitted</div>
                                                    {{else}}
                                                    <div class="badge badge-warning">Not Submitted</div>
                                                    {{/if}}
                                                </td>
                                                {{!-- <td class="text-center">
                                                <a><button type="button" id="PopoverCustomT-1" class="btn btn-primary btn-sm">Details</button></a>
                                                </td> --}}
                                            </tr>
                                            {{/each}}
                                    </tbody>
                                        </table>
                                    </div>
                                    <div class="d-block text-center card-footer">
                                    {{#if attemptData.AttemptPagination.PrevPage}}
                                        <a href="?page={{attemptData.AttemptPagination.PrevPage}}"><button class="btn-wide btn btn-primary">Prev</button></a>                                      
                                    {{/if}}
                                    {{#if attemptData.AttemptPagination.NextPage}}
                                        <a href="?page={{attemptData.AttemptPagination.NextPage}}"><button class="btn-wide btn btn-primary">Next</button></a>
                                    {{/if}}
                                    </div>
                                </div>
                            </div>
                        </div>



                    
